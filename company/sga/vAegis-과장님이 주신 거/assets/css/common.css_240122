	--malware-color:#2ADB6B;
	--malware-t-color:#24413D;

	--critical-color:#C1282E;
	--critical-t-color:#6F4831;
	--hight-color:#F7941F;
	--hight-t-color:#433631;
	--medium-color:#FFD52F;
	--medium-t-color:#4B404F;
	--low-color:#39B549;
	--low-t-color:#283F3A;

	--success-color:#4863FC;
	--success-t-color:#233253;

	--passed-color:#55BC55;
	--passed--t-color:#2B3C3A;

	--fail-color:#E01E5F; 
	--fail-t-color:#41243C;

	--sc-success-color:#79D2DE;
	--sc-success-t-color:#315657;
	--sc-medium-color:#2ADB77; 
	--sc-medium-t-color:#24413F;
	--sc-scanning-color:#1783FF; 
	--sc-scanning-t-color:#1F5982;

	--sensitive-color:#006EFD;

	--image-color:#9821FF;
	--image-t-color:#352553;
	--assurance-color:#FF21AE; 
	--assurance-t-color:#6A245C;

	
	--none-color:#b2b2b2;
	--none-t-color:#4B4C56;
	--erro-color:#ff7840;
	

	
}

/**위험도 등급 컬러*/

.bc_malware{ background-color:var(--malware-color); color:#fff;  }/**멀웨어**/
.tc_malware{ color:var(--malware-color); }

.bc_sensitive{ background-color:var(--sensitive-color); color:#fff;  }/**민감도 정보, Sensitive Data**/
.tc_sensitive{ color:var(--sensitive-color); }

.bc_image{ background-color:var(--image-color); color:#fff;  }/**미보증 이미지, image Assurance**/
.tc_image{ color:var(--image-color); }/**미보증 이미지, image Assurance**/

.bc_detection{ background-color:var(--image-color); color:#fff;  }/**Detection Event**/
.tc_detection{ color:var(--image-color); }/**Detection Event**/

.bc_critical{ background-color:var(--critical-color) !important; }
.tc_critical{ color:var(--critical-color) !important; }

.bc_high{ background-color:var(--hight-color) !important;  }
.tc_high{ color:var(--hight-color) !important; }

.bc_medium{ background-color:var(--medium-color); color:#716924 !important;  }
.tc_medium{ color:var(--medium-color) !important; }

.bc_low{ background-color:var(--low-color) !important;  }
.tc_low{ color:var(--low-color) !important; }

.bc_info{ background-color:var(--btn-color-blue) !important;  }
.tc_info{ color:var(--btn-color-blue) !important;  }

.bc_passed,
.passed_cnt_box{ background-color:var(--passed-color) !important;   }
.tc_passed{ color:var(--passed-color) !important; }
.bc_failed,
.failed_cnt_box{ background-color:var(--fail-color) !important; color:#fff; }
.tc_failed{ color:var(--fail-color) !important; }



.non,
.bc_ignore,
.etc_cnt_box{ background-color:var(--none-color) !important;  }
.tc_non{ color:var(--none-color) !important;  }

.bc_erro,
.error_cnt_box{ background-color:var(--erro-color) !important; color:#fff;  }
.tc_erro{ color:var(--erro-color) !important;  }







/*malware*/
.malware .chart > div::before{ background:var(--malware-t-color); box-shadow: 0 0 0 1px var(--malware-color) inset; }
.malware .chart > div > p,
.malware .chart > div > p span.value{ transform:rotate(0); }
.malware .chart > div > p::after { background:var(--malware-t-color);   }
.malware .chart > div > p span.value { top:50%; right:-20px; }
.malware p.annex span.malware::before { background:var(--malware-color);   }

/**vulnerabilities*/
.vulnerabilities .chart > div > p,
.vulnerabilities .chart > div > p span.value{ transform:rotate(0); }
.vulnerabilities .chart > div > p::after { background:#24413D;   }
.vulnerabilities .chart > div > p span.value { top:50%; right:-20px; }

.vulnerabilities .chart > div > p.hight-chart{ transform:rotate(45deg); }
.vulnerabilities .chart > div > p.hight-chart span.value{ transform:rotate(-45deg); }

.vulnerabilities .chart > div > p.critical-chart{ transform:rotate(180deg); }
.vulnerabilities .chart > div > p.critical-chart span.value{ top:25%; transform:rotate(0); }

.vulnerabilities .chart > div > p.medium-chart{ transform:rotate(0); }
.vulnerabilities .chart > div > p.medium-chart span.value{ top:0; left:80px; right:inherit; transform:rotate(90deg); }

.vulnerabilities .chart > div > p.low-chart{ transform:rotate(130deg); }
.vulnerabilities .chart > div > p.low-chart span.value{ top:0; left:80px; right:inherit; transform:rotate(90deg); }


.vulnerabilities .chart > div.low::before{ background:var(--low-t-color); box-shadow: 0 0 0 1px var(--low-color) inset; }
.vulnerabilities .chart > div.hight::before{ background:var(--hight-t-color); box-shadow: 0 0 0 1px var(--hight-color) inset; }
.vulnerabilities .chart > div.critical::before{ background:var(--critical-t-color); box-shadow: 0 0 0 1px var(--critical-color) inset; }
.vulnerabilities .chart > div.medium::before{ background:var(--medium-t-color); box-shadow: 0 0 0 1px var(--medium-color) inset; }
.vulnerabilities .chart > div.ignore::before{ background:var(--none-t-color); box-shadow: 0 0 0 1px var(--none-color) inset; }

.vulnerabilities .chart > div > p.hight-chart::after{ background:var(--hight-t-color); }
.vulnerabilities .chart > div > p.critical-chart::after{ background:var(--critical-t-color); }
.vulnerabilities .chart > div > p.medium-chart::after{ background:var(--medium-t-color); }
.vulnerabilities .chart > div > p.low-chart::after{ background:var(--low-t-color); }
.vulnerabilities .chart > div > p.ignore-chart::after{ background:var(--none-t-color); }

--none-t-color

.vulnerabilities p.annex span.critical::before { background:var(--critical-color);;   }
.vulnerabilities p.annex span.hight::before{ background:var(--hight-color);   }
.vulnerabilities p.annex span.medium::before { background:var(--medium-color);   }
.vulnerabilities p.annex span.ignore::before { background:var(--none-color);   }


/**Scan*/
.scan .chart > div > p,
.scan .chart > div > p span.value{ transform:rotate(0); }
.scan .chart > div > p::after { background:#24413D;   }
.scan .chart > div > p span.value { top:50%; right:-20px; }

.scan .chart > div > p.success-chart{ transform:rotate(45deg); }
.scan .chart > div > p.success-chart span.value{  transform:rotate(-45deg); }

.scan .chart > div > p.fail-chart{ transform:rotate(-90deg); }
.scan .chart > div > p.fail-chart span.value{ top:0; left:80px; right:inherit; transform:rotate(90deg); }


.scan .chart > div.success::before{ background:var(--success-t-color); box-shadow: 0 0 0 1px var(--success-color) inset; }
.scan .chart > div.fail::before{ background:var(--fail-t-color); box-shadow: 0 0 0 1px var(--fail-color) inset; }
.scan .chart > div > p.success-chart::after{ background:var(--success-t-color); }
.scan .chart > div > p.fail-chart::after{ background:var(--fail-t-color); }
.scan p.annex span.success::before { background:var(--success-color);  }
.scan p.annex span.fail::before{ background:var(--fail-color);   }


/**scan Completion*/
.scan_completion .chart > div > p,
.scan_completion .chart > div > p span.value{ transform:rotate(0); }
.scan_completion .chart > div > p::after { background:#24413D;   }
.scan_completion .chart > div > p span.value { top:50%; right:-20px; }

.scan_completion .chart > div > p.sc-success-chart{ transform:rotate(45deg); }
.scan_completion .chart > div > p.sc-success-chart span.value{ right:0; transform:rotate(-45deg); }

.scan_completion .chart > div > p.sc-medium-chart{ transform:rotate(-90deg); }
.scan_completion .chart > div > p.sc-medium-chart span.value{ top:-10px; left:110px; right:inherit; transform:rotate(90deg); }


.scan_completion .chart > div.sc-success::before{ background:var(--sc-success-t-color); box-shadow: 0 0 0 1px var(--sc-success-color) inset; }
.scan_completion .chart > div.sc-medium::before{ background:var(--sc-medium-t-color); box-shadow: 0 0 0 1px var(--sc-medium-color) inset; }
.scan_completion .chart > div > p.sc-success-chart::after{ background:var(--sc-success-t-color); }
.scan_completion .chart > div > p.sc-medium-chart::after{ background:var(--sc-medium-t-color); }
.scan_completion p.annex span.sc-success::before { background:var(--sc-success-color);  }
.scan_completion p.annex span.sc-medium::before{ background:var(--sc-medium-color);   }


/**Image Assurance */
.image_assurance .chart > div > p,
.image_assurance .chart > div > p span.value{ transform:rotate(0); }
.image_assurance .chart > div > p::after { background:#24413D;   }
.image_assurance .chart > div > p span.value { top:50%; right:-20px; }

.image_assurance .chart > div > p.image-chart{ transform:rotate(45deg); }
.image_assurance .chart > div > p.image-chart span.value{ right:0; transform:rotate(-45deg); }

.image_assurance .chart > div > p.assurance-chart{ transform:rotate(-90deg); }
.image_assurance .chart > div > p.sc-medium-chart span.value{ top:-10px; left:110px; right:inherit; transform:rotate(90deg); }


.image_assurance .chart > div.image::before{ background:var(--image-t-color); box-shadow: 0 0 0 1px var(-image-color) inset; }
.image_assurance .chart > div.assurance::before{ background:var(--assurance-t-color); box-shadow: 0 0 0 1px var(--assurance-color) inset; }
.image_assurance .chart > div > p.image-chart::after{ background:var(--image-t-color); }
.image_assurance .chart > div > p.assurance-chart::after{ background:var(--assurance-t-color); }
.image_assurance p.annex span.image::before { background:var(--image-color);  }
.image_assurance p.annex span.assurance::before{ background:var(--assurance-color);   }




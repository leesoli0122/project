/**툴팁**/
.tolltip{ display:none; position:absolute; z-index:3; left:340px; top:50%; right:20px; transform:translateY(-50%); padding:0; background:#ffffffeb; border-radius:20px; box-shadow:1px 1px 1px #000; }
.tolltip *{ color:#121212; }
.tolltip.on{ display:block; }
.tolltip::before {content: ''; position: absolute; left: -19px; top: 50%;
  margin-top: -10px;
  border-top: 10px solid transparent;
  border-left: 10px solid transparent;
  border-bottom: 10px solid transparent;
  border-right: 10px solid #ffffffeb;

}

.tolltip dt{ padding:0 20px; display:flex; justify-content:center; align-items:center; width:100%; min-height:48px; border-bottom:1px solid #707070; }
.tolltip dd{ padding:10px 20px; }
.tolltip dd.flex{ display:flex; padding:30px 20px; }
.tolltip dd.flex div { display:flex; flex-direction:column; justify-content:center; }

.tolltip dd.flex div:first-child{ width:200px; padding:0 20px 0 0; }
.tolltip dd.flex div:last-child{ width:calc(100% - 200px); }
.tolltip p{ display:block; position:relative; padding-top:10px; }
.tolltip dd.flex div:first-child p{ padding-top:0; font-weight:800; font-size:38px; height:90px; line-height:90px; color:#fff; background:var(--card-color01); border-radius:15px; text-align:center; }
.tolltip label{ font-weight:600; display:inline-block; padding-left:7px; }
.tolltip label::before{ content:""; display:inline-block; position:absolute; top:15px; left:0; width:5px; height:5px; background:#121212; border-radius:30px; }
.tolltip label::after{ content:":"; display:inline; margin:0 2px 0 5px; }
.tolltip label.block{ display:block; margin-bottom:3px; }
.tolltip label.block::after{ display:none; }

.tbl{ position:relative; }
.progressbarBox.relative{ position:relative; padding:0; top:0; }
.progressbarBox{ background:#121212b5; width:auto; position:absolute; top:50px; left:10px; bottom:0; right:10px;  }
.progressbarBox .Loading{
  width:96px; height:16px; display:inline-block; border:4px solid #FFF; border-radius:7px;
 background-image:linear-gradient(-45deg, rgb(46, 111, 212) 25%, transparent 25%, transparent 50%, rgb(46, 111, 212) 50%, rgb(46, 111, 212) 75%, transparent 75%, transparent);
  font-size: 30px;
  background-size: 1rem 1rem;
  box-sizing: border-box;
  animation: barStripe 1s linear infinite;
}

.progressbar p{ color:#fff; font-weight:500; font-size:14px; }
.progressbar .Loading span{ background:transparent; }

@keyframes barStripe {
  0% {
    background-position: 1rem 0;
  }
  100% {
    background-position: 0 0;
  }
}




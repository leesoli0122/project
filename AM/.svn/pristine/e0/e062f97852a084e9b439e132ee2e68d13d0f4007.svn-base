@charset "UTF-8";

/*reset */
html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video{border:0;font-size:100%;font:inherit;vertical-align:top;margin:0;padding:0}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}
body{font-size:87.5%;font-weight:normal;line-height:1;}
ol,ul{list-style:none;}
blockquote,q{quotes:none;}
blockquote:before,blockquote:after,q:before,q:after{content:none;}
table{border-collapse:collapse;border-spacing:0;}
button,select{vertical-align:middle;-webkit-appearance: none;-moz-appearance:none;appearance:none;}
select::-ms-expand {display:none} /* ie 10,11 화살표 숨기기 */
input,button {border:0 none;background-color:#fff;}
select,button {text-transform: none}
label {cursor:pointer;}
.blind, legend, hr, caption{display:block;overflow:hidden;position:absolute;top:0;left:-1000em;}
i, em, address {font-style:normal;font-weight:normal;}
input[type="button"]{cursor:pointer;}
a, button {cursor:pointer;}
a {text-decoration:none;}
button {padding:0;outline:none;}

/***********Page setting*************/
html, body {width:100%;height:100%;}
#wrap {min-width:1024px;height:100%;}
body {background:#f4f5f7;}
/* 폰트 */
@font-face{
	font-family:'Noto Sans CJK KR';font-style:normal;font-weight:300;
	src:url('../../font/NotoSans-Light.eot');
	src:url('../../font/NotoSansKR-Light.woff2')format('woff2'),
	url('../../font/NotoSansKR-Light.woff')format('woff'),
	url('../../font/NotoSansKR-Light.otf')format('opentype');
}
@font-face{
	font-family:'Noto Sans CJK KR';font-style:normal;font-weight:400;
	src:url('../../font/NotoSans-Regular.eot');
	src:url('../../font/NotoSansKR-Regular.woff2')format('woff2'),
	url('../../font/NotoSansKR-Regular.woff')format('woff'),
	url('../../font/NotoSansKR-Regular.otf')format('opentype');
}
@font-face{
	font-family:'Noto Sans CJK KR';font-style:normal;font-weight:500;
	src:url('../../font/NotoSans-Medium.eot');
	src:url('../../font/NotoSansKR-Medium.woff2')format('woff2'),
	url('../../font/NotoSansKR-Medium.woff')format('woff'),
	url('../../font/NotoSansKR-Medium.otf')format('opentype');
}
@font-face{
	font-family:'Noto Sans CJK KR';font-style:normal;font-weight:600;
	src:url('../../font/NotoSans-Bold.eot');
	src:url('../../font/NotoSansKR-Bold.woff2')format('woff2'),
	url('../../font/NotoSansKR-Bold.woff')format('woff'),
	url('../../font/NotoSansKR-Bold.otf')format('opentype');
}

body, div, input, button {font-family:'Noto Sans CJK KR','Noto Sans KR','Helvetica Neue',Helvetica,'Malgun Gothic','Dotum', sans-serif !important;color:#5f5f5f;font-weight:400}
a {color:#5f5f5f;text-decoration:none}
a:hover, a:focus {color:#5f5f5f;text-decoration:none}

/*** layout ***/
/*공통*/
.hidden {text-indent:-5000px;width:0;height:0;font-size:0;line-height:0;}
.f_l {float:left !important;}
.f_r {float:right !important;}
.table-cell {display:table-cell;vertical-align:middle;}
.table-row {display:table-row;}
.box2:after,.box3:after,.box4:after {content:"";display:block;clear:both}
#container {margin-left:242px;padding-top:60px;padding-bottom:30px;transition:all 0.4s}
#container.ml0 {margin-left:0px;transition:all 0.4s}
.content {padding:24px;position:relative;}
.lnb_wrap,#container,#footer {box-sizing:border-box;}
.lnb_wrap,.content .title_box,.content .tbl_area,.content .list_detail {background-color:#fff;border:1px solid #e1e4e9;box-sizing:border-box;}
.pd24 {padding:24px 24px 30px;}
.tit {font-weight:500;}
.tit02 {font-size:1.143em;border-bottom:1px solid #eaeaea;padding-bottom:12px}
.wid100 {width:100%;box-sizing:border-box}
.wid50 {width:50%;box-sizing:border-box;}
.wid33 {width:33.333%;box-sizing:border-box;}
.wid25 {width:25%;box-sizing:border-box;}
.otp_btn_box {margin-top:26px;padding-top:22px;border-top:1px solid #d0d3d7;}
.flx {display:flex;}
.row_devide {width:100%;height:1px;border-top:1px solid #e0e0e0}
.pointtxt {color:#466fb6}
.redtxt {color:#E84B1A}
.i-card_wrap {display:flex;padding-bottom:15px;gap:15px}
.dash_box {background-color:#fff;border:1px solid #e1e4e9;flex-grow:1;}

/* 세로 scroll */
.lnb_wrap::-webkit-scrollbar,.tree_wrap::-webkit-scrollbar,
.popup_content::-webkit-scrollbar {width:6px;height:6px;}
.lnb_wrap::-webkit-scrollbar-track,.tree_wrap::-webkit-scrollbar-track,
.popup_content::-webkit-scrollbar-track {border-radius:10px;background:#fafafa;box-shadow:inset 0 0 6px rgba(0,0,0,0.3)}
.lnb_wrap::-webkit-scrollbar-thumb,.tree_wrap::-webkit-scrollbar-thumb,
.popup_content::-webkit-scrollbar-thumb {border-radius:10px;background:#bbb;}
.lnb_wrap::-webkit-scrollbar-thumb:window-inactive,
.tree_wrap::-webkit-scrollbar-thumb:window-inactive,
.popup_content::-webkit-scrollbar-thumb {background:rgba(100,100,100,0.4);}

.scroll {overflow-y:auto}
.scroll::-webkit-scrollbar {width:6px;height:6px;}
.scroll::-webkit-scrollbar-track {border-radius:10px;background:#fafafa;box-shadow:inset 0 0 6px rgba(0,0,0,0.1)}
.scroll::-webkit-scrollbar-thumb {border-radius:10px;background:#cbcbcd;}
.scroll::-webkit-scrollbar-thumb:window-inactive {background:rgba(100,100,100,0.4);}

/* 가로 scroll */
.ui-jqgrid .ui-jqgrid-bdiv::-webkit-scrollbar,
.media_wrap::-webkit-scrollbar {height:8px;}
.ui-jqgrid .ui-jqgrid-bdiv::-webkit-scrollbar-track,
.media_wrap::-webkit-scrollbar-track {background:#eee;}
.ui-jqgrid .ui-jqgrid-bdiv::-webkit-scrollbar-thumb,
.media_wrap::-webkit-scrollbar-thumb {background:#bbb;}
.ui-jqgrid .ui-jqgrid-bdiv::-webkit-scrollbar-thumb:window-inactive,
.media_wrap::-webkit-scrollbar-thumb:window-inactive {background:rgba(100,100,100,0.4);}

/* top */
#header .gnb {width:100%;min-width:768px;height:60px;position:fixed;top:0;left:0;z-index:2000;background-color:#466fb6;padding:0 10px;box-sizing: border-box;}
.gnb a {display:block;}
.gnb a, .gnb p {color:#fff;}
.gnb .logo a {position:absolute;top:0;left:50%;width:300px;height:60px;margin-left:-150px;background:url('../../img/pm/gnb_logo.png') 50% 50% no-repeat;text-indent: -5000px}

.gnb .logo_type02 {display:flex;align-items:center;gap:6px;position:absolute;top:0;left:50%;margin-left:-144px}
.gnb .logo_type02 a {width:188px;height:60px;background:url('../../img/pm/gnb_logo_type02.png') 50% 50% no-repeat;text-indent: -5000px}
.gnb .logo_type02 .logo_settxt {background-color:#fff;border-radius:12px;color:#466FB6;padding:1px 4px;font-size:11px;height:12px;margin-bottom:7px}

.gnb .btn_tree,.gnb .btn_allcategory,.gnb .user_area li,.gnb .btn_lnb  {float:left;}
.gnb .btn_tree a,.gnb .btn_allcategory a,.gnb .btn_lnb a,.gnb .user_area li a {height:60px;padding:0 14px;}
.gnb .btn_tree a:hover,.gnb .btn_allcategory a:hover,.gnb .btn_lnb a:hover,.gnb .user_area li a:hover,.gnb .btn_allcategory a.on,.gnb .user_area li a.on {transition: all 0.3s ease-out;background-color:#355da2}
.gnb .btn_tree a.on {background-color:#355da2;}
.gnb:after,.gnb .user_area {display:block;content:"";clear:both;}
.gnb .btn_tree a {background:url('../../img/pm/icon_group.png') 14px center no-repeat;padding-left:45px;line-height:60px;}
.gnb .btn_allcategory a,.gnb .btn_lnb a {width:19px;position:relative;}
.gnb .btn_allcategory a {display:none;}
.gnb .btn_allcategory a .icon,.gnb .btn_lnb a .icon {display:block;width:19px;height:15px;position:absolute;top:50%;left:50%;margin-top:-6px;margin-left:-8px;background: url('../../img/pm/icon_allcategory.png') 0 0 no-repeat;text-indent: -5000px}
.gnb .btn_lnb a .icon {background-image:url('../../img/pm/icon_lnb.png');margin-top:-7px;}
.gnb .btn_allcategory a.on .icon {background-position:0 -16px;}
.gnb .btn_lnb a.on .icon {background-position:0 -15px;}

.gnb .user_area {position:absolute;top:0;right:10px;}
.gnb .user_area li a {width:20px;position:relative;}
.gnb .user_area li a .icon {display:block;width:20px;height:20px;position:absolute;top:50%;left:50%;margin-top:-10px;margin-left:-10px;background: url('../../img/pm/icon_user.png') 0 0 no-repeat;text-indent: -5000px}
.gnb .user_area .btn_user .icon {margin-left:-9px;}
.gnb .user_area .btn_alam .icon {background-position:-19px 0;}
.gnb .user_area .btn_alam span {display:block;position:absolute;top:13px;left:25px;width:18px;height:18px;font-size:10px;border-radius: 50%;background-color:#e64a19;text-align: center;line-height: 18px}
.gnb .user_area .btn_logout .icon {background-position:-39px 0;}
.gnb .user_area .btn_download .icon {background-position:-60px 0;}
.gnb .user_area .btn_server .icon {background-position:-80px 0;margin-left:-8px}
.gnb .user_area .btn_PMSSO a .icon {width:24px;background:url('../../img/pm/btn_PM.png') -1px 50% no-repeat;}
.gnb .user_area .btn_PMsync a .icon {width:25px;background:url('../../img/pm/btn_PMsync.png') 0px 50% no-repeat;}
.gnb .user_area .btn_IM a .icon {width:25px;background:url('../../img/prime/btn_IM.png') 0px 50% no-repeat;}
.gnb .user_area .btn_AM a {width:31px;}
.gnb .user_area .btn_AM a .icon {width:31px;margin-left:-15px;background:url('../../img/prime/btn_AM.png') -1px 50% no-repeat;}
.gnb .user_area .btn_ICAM a {width:40px}
.gnb .user_area .btn_ICAM a .icon {width:40px;background:url('../../img/prime/btn_ICAM.png') 0px 50% no-repeat;margin-left:-20px}

/***************** lnb *******************/
/* 유저정보 */
.lnb_wrap,.tree_wrap {width:242px;height:100%;position:fixed;left:0;top:60px;bottom:0;overflow-x:hidden;overflow-y:auto;border:0 none;border-right:1px solid #e2e2e2;transition:all 0.4s}
.tree_wrap {border-right-color:#ccd2d2;}
.lnb_wrap.left0 {left:-242px;transition:all 0.4s}
.lnb_wrap .user_info {padding:30px 10px 30px 20px;border-bottom:1px solid #f4f6f9}
.lnb_wrap .user_info:after {content: "";display: block;clear:both;}
.lnb_wrap .user_info .profile_img {float:left;width:30%;}
.lnb_wrap .user_info .profile_img img {width:57px;height:57px;border-radius:50%}
.lnb_wrap .user_info .text_info {float:right;width:70%;padding-left:10px;box-sizing: border-box;}
.lnb_wrap .user_info .text_info .user_id {font-weight:bold}
.lnb_wrap .user_info .text_info dl {padding-top:14px;}
.lnb_wrap .user_info .text_info dt {text-indent:-5000px;width:11px;height:13px;display:inline-block;*display:inline;zoom:1;background:url('../../img/pm/icon_time_ip.png') 0 0 no-repeat;}
.lnb_wrap .user_info .text_info dt.icon_logintime {background-position:center 2px}
.lnb_wrap .user_info .text_info dt.icon_account {background-position:center -17px}
.lnb_wrap .user_info .text_info dd {padding:0 0 6px 2px;font-size:0.857em;display:inline-block;line-height:1.2}
/* nav */
.main_menu {padding-bottom:60px;}
.main_menu>ul>li>a {display:block;position:relative;padding:20px 24px;padding-left:54px;font-weight:500;color:#444;}
.main_menu>ul>li.on>a, .main_menu>ul>li>a:hover {background-color:#f2f2f2;color:#466fb6;}
.main_menu li ul {display:none;background-color:#f2f2f2;border-bottom:1px solid #e4e4e4;}
.main_menu>ul>li>ul>li>a {display:block;padding:12px 24px 12px 64px;font-weight:400;}
.main_menu>ul>li>ul>li>a.on {background:url('../../img/pm/depth2_on.png') right center no-repeat;}
.main_menu>ul>li>ul>li>a.on,.main_menu>ul>li>ul>li>a:hover {background-color:#dee0e3;color:#466fb6;}
.main_menu>ul>li>a .menu_arrow {position:absolute;right:24px;top:50%;margin-top:-3px;display:inline-block;width:9px;height:6px;background:url('../../img/pm/nav_arrow.png') no-repeat 0 bottom;}
.main_menu>ul>li.on>a .menu_arrow {background-position:0 top;}
.main_menu>ul>li.on>ul{display:block}

/* nav icon */
.nav_icon>ul>li>a>em {position:absolute;left:24px;top:50%;margin-top:-8px;display:inline-block;width:19px;height:19px;background-repeat:no-repeat;background-position:0 top}
.nav_icon>ul>li>a .icon_home {background-image:url('../../img/pm/icon_home.png')}
.nav_icon>ul>li>a .icon_mng {background-image:url('../../img/pm/icon_mng.png')}
.nav_icon>ul>li>a .icon_access {background-image:url('../../img/pm/icon_access.png')}
.nav_icon>ul>li>a .icon_workflow {background-image:url('../../img/pm/icon_workflow.png')}
.nav_icon>ul>li>a .icon_schedule {background-image:url('../../img/pm/icon_schedule.png')}
.nav_icon>ul>li>a .icon_report {background-image:url('../../img/pm/icon_report.png')}
.nav_icon>ul>li>a .icon_report02 {background-image:url('../../img/pm/icon_report02.png')}
.nav_icon>ul>li>a .icon_setting {background-image:url('../../img/pm/icon_setting.png')}
.nav_icon>ul>li>a .icon_imlink {background-image:url('../../img/pm/icon_imlink.png')}
.nav_icon>ul>li>a .icon_password {background-image:url('../../img/pm/icon_password.png')}
/*nav icon-on*/
.nav_icon>ul>li.on>a>em, .main_menu>ul>li>a:hover>em {background-position:0 bottom}


/* 전체카테고리 보기 */
.nav_allcategory {width:700px;box-sizing:border-box;background-color:#fff;box-shadow:0 0 1px 2px rgba(80,80,80,0.3);position:fixed;top:60px;left:120px;padding:24px 24px 4px 30px;z-index:1000}
.gnb .btn_allcategory a.on:after,.layer_alim:after,.layer_menual:after,.layer_serverstatus:after {position:absolute;top:-7px;left:18px;display:block;content: "";width:0;height:0;border-left:6px solid transparent;border-right:6px solid transparent;border-bottom:7px solid #fff}
.gnb .btn_allcategory a.on:after {top:auto;bottom:0;z-index:1001}
.nav_allcategory>ul>li {width:100%;border-bottom:1px solid #f4f4f4;margin-bottom:20px;}
.nav_allcategory>ul>li:last-child {border-bottom:0 none;margin-bottom:0;}
.nav_allcategory>ul>li:after,.nav_allcategory>ul>li>ul:after {content:"";display:block;clear:both}
.nav_allcategory>ul>li>a {display:block;float:left;position:relative;width:35%;box-sizing:border-box;padding-left:28px;font-size:1.143em;color:#466fb6;font-weight:400}
.nav_allcategory>ul>li>a>em {left:0}
.nav_allcategory>ul>li>ul {float:left;width:65%;}
.nav_allcategory>ul>li>ul>li {float:left;width:33.333%;padding-bottom:20px;}
.nav_allcategory>ul>li>ul>li a {line-height:1.4;padding-left:12px;background:url('../../img/pm/dot.png') no-repeat 0 50%;}

/* 알림&메뉴얼&서버상태 */
.layer_alim,.layer_menual,.layer_serverstatus {box-sizing:border-box;z-index:2000;background-color:#fff;box-shadow:0 0 4px 2px rgba(80,80,80,0.3);position:fixed;top:60px;right:58px;padding:0 20px;}
.layer_alim,.layer_menual {width:190px;}
.layer_menual {right:105px;}
.layer_serverstatus {right:153px;min-width:190px;}
.layer_alim:after,.layer_menual:after,.layer_serverstatus:after {left:auto;right:18px;}
.layer_alim ul li a,.layer_menual ul li a {display:block;position:relative;padding:17px 40px 17px 0;border-bottom:1px solid #e7e8e9;}
.layer_serverstatus ul li {padding:17px 0;border-bottom:1px solid #e7e8e9;}
.layer_alim ul li:last-child a,.layer_menual ul li:last-child a,.layer_serverstatus ul li:last-child {border-bottom:0 none;}
.layer_alim ul li a span {display:inline-block;position:absolute;top:15px;right:0;}
.layer_alim ul li a span em {font-size:1.2em;font-weight:500;color:#466fb6;vertical-align:bottom}
.layer_alim ul li a i,.layer_menual ul li a i {display:inline-block;width:16px;height:16px;margin-right:8px;}
.layer_alim ul li a i {background:url('../../img/pm/icon_alim.png') no-repeat 0 0;background-size:32px}
.layer_alim ul li a i.icon_pmcomple {background-position:-16px 0;}
.layer_alim ul li a i.icon_pmreq {background-position:0 -16px;}
.layer_alim ul li a i.icon_wkcomple {background-position:-16px -16px;}
.layer_menual ul li a i {background:url('../../img/pm/icon_menual.png') no-repeat 0 0}
.layer_serverstatus li dl:after {content:"";display:block;clear:both;}
.layer_serverstatus li dt {float:left;}
.layer_serverstatus li dd {float:right;font-weight:500;margin-left:10px;}
.layer_serverstatus li dd .host_txt {display:inline-block;max-width:150px;word-break:break-all;text-align:right}
.layer_serverstatus li dd.stb_txt {padding:4px 10px;color:#fff;border-radius:12px;margin-top:-4px;}
.layer_serverstatus li dd.status_normal {background-color:#466fb6;}
.layer_serverstatus li dd.status_standby {background-color:#eaaf0f;}
.layer_serverstatus li dd.status_down {background-color:#de704a;}
.layer_serverstatus li dd.status_unknown {background-color:#747676;}

/* 그룹트리 */
.tree_wrap {z-index:1000;background-color:#f6f9f9;}
.tree_wrap .tab {padding:0 4px;background-color:#fff}
.tree_wrap .tab ul:after {content:"";display:block;clear:both;}
.tree_wrap .tab ul li {float:left;width:33.333%;}
.tree_wrap .tab ul li a {display:block;box-sizing:border-box;padding:20px 0 17px 0;margin:0 14px;font-weight:500;text-align:center;}
.tree_wrap .tab ul li a.on {border-bottom:3px solid #466fb6;color:#466fb6;}
.tree_wrap .tree_box {border-top:1px solid #e8e8e8;padding:20px 16px;}

.tree_view {padding:14px 0;}
.tree_view li a {line-height:24px;height:24px;padding:2px 4px;font-size:0.929em;}
.tree_view li a:hover,.tree_view li a:focus {background-color:#ebf6f6;border-radius:2px;box-shadow:inset 0 0 1px #5fb3b7;color:#000;}
.tree_view span {display:inline-block;width:24px;height:24px;margin-right:2px;background:url('../../img/pm/icon_folder.png') no-repeat 0 0;}
.tree_view .icon_folder_open {background-position:0 0}
.tree_view .icon_folder_close {background-position:0 -25px}
.tree_view .icon_file {background-position:0 -48px}
.tree_view ul ul {padding-left:26px;}
.tree_view li {margin:2px 0;}

/************ form ***************/
.form_contrl {width:100%;height:34px;border-radius:3px;border:1px solid #AEAEAE;box-sizing:border-box;padding:0 10px;font-size:0.857em;color:inherit;box-shadow:none;font-size:0.929em;}
.form_contrl:focus {border-color:#466fb6;outline:0}
select.form_contrl {background:url('../../img/pm/select_arrow.png') no-repeat right 50% #fff}
textarea.form_contrl {font-size:0.929em;font-family:'Noto Sans CJK KR','Noto Sans KR';padding:5px 10px;}
.form_contrl.diss {background-color:#fbfbfb;}
.content .default_tbl .form_contrl {width:100%;}
.s_required label:after,.s_required span:after {display:inline-block;content:"";width:9px;height:9px;background:url('../../img/pm/s_req.png') no-repeat right 0;margin-left:5px;}
.req_txt {font-size:0.9em;padding-top:5px;color:#ea8b36;}
.req_txt.nxtto_cr {padding:0 0}
.fas-w {position:relative;}
.fas {display:block;position:absolute;top:0;right:0;width:34px;height:34px;cursor:pointer;}
.fas:after {content:"";display:block;position:absolute;width:17px;height:17px;top:50%;left:50%;margin:-9px 0 0 -9px;background:url('../../img/pm/icon_eye.png') no-repeat 0 0;}
.fas.hide:after {background-position-y:-17px;}
.tbl_list .form_contrl {height:30px}


/*checkbox*/
.popup_content .checkbox_style {padding-left:20px;}
.checkbox_style input[type="radio"],.checkbox_style input[type="checkbox"] {opacity:0;z-index:1;width:0;height:0;margin:0;outline:none !important}
.checkbox_style label {position:relative;padding-left:2px;}
.checkbox_style label:before {background-color:#fff;border-radius:3px;border:1px solid #b4baba;content:"";display:inline-block;width:16px;height:16px;position:absolute;top:3px;left:0;transition:0.3s ease-in-out;margin-left:-20px;box-sizing:border-box;}
.checkbox_style input[type="radio"] + label:before {border-radius:50%}
.checkbox_style input[type="checkbox"]:checked + label:before,.checkbox_style input[type="radio"]:checked + label:before {background-color:#466fb6;border:0 none;}
.checkbox_style input[type="checkbox"]:checked + label:after,.checkbox_style input[type="radio"]:checked + label:after {content:"";display:block;width:16px;height:16px;background:url('../../img/pm/checked.png') no-repeat 0 0;position:absolute;top:3px;left:0;margin-left:-20px;}
.checkbox_style input[type="radio"]:checked + label:after {width:6px;height:6px;background-image:none;background-color:#fff;border-radius:50%;top:8px;left:5px}
.checkbox_style.check_center label:before,.checkbox_style.check_center input[type="checkbox"]:checked + label:after,.checkbox_style.check_center input[type="radio"]:checked + label:after {left:50%;margin-left:-8px;}
.checkbox_style.check_center input[type="radio"]:checked + label:after {margin-left:-3px;}
.checkbox_style.flx p {margin-right:30px}

.form_wrap dl,.wk_choice dl {position:relative;}
.form_wrap dt,.wk_choice dt {position:absolute;left:0;padding-top:22px;width:150px;font-weight:500;}

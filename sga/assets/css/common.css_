.ui_01_04_01 .card_box .card_list li > p{ display:block; width:100%; height:45px; line-height:55px; text-align:right;  }
.ui_01_04_01 .card_box .card_list li.malware p{ background:url("../images/common/ico_malware.png") no-repeat left center; background-size:auto 45px; }
.ui_01_04_01 .card_box .card_list li.sensitive p{ background:url("../images/common/ico_sensitive.png") no-repeat left center; background-size:auto 45px; }
.ui_01_04_01 .card_box .card_list li.image p{ background:url("../images/common/ico_image.png") no-repeat left center; background-size:auto 45px; }
.ui_01_04_01 .card_box .card_list li.detection p{ background:url("../images/common/ico_detection.png") no-repeat left center; background-size:auto 45px; }

/*컨테이너 워크로드 현황-상세정보 - Vulnerabilities*/
.card_box.vulnerabilities_info .card_list li{ position:relative; }
.card_box.vulnerabilities_info .card_list li{ background-repeat:no-repeat; background-position:15px 10px;  }
.card_box.vulnerabilities_info .card_list li.tc_critical{ background-image:url("../images/common/ico_critical.png"); }
.card_box.vulnerabilities_info .card_list li.tc_high { background-image:url("../images/common/ico_high.png"); }
.card_box.vulnerabilities_info .card_list li.tc_medum { background-image:url("../images/common/ico_medum.png"); }
.card_box.vulnerabilities_info .card_list li.tc_row { background-image:url("../images/common/ico_low.png"); }
.card_box.vulnerabilities_info .card_list li.tc_info { background-image:url("../images/common/ico_info.png"); }
.card_box.vulnerabilities_info .card_list li.tc_non { background-image:url("../images/common/ico_unknown.png"); }
.card_box.vulnerabilities_info .card_list li p { margin-top:10px; }
.card_box.vulnerabilities_info li span{ color:inherit;  }
.card_box.vulnerabilities_info li span.label{ display:inline; padding:0; margin:0; text-align:right; font-weight:500;  }

/*클러스터 규정준수 스캔*/
.ui_01_02{ min-height:267px !important; height:245px; margin-bottom:0 !important; }
.gauge_box{ padding:30px 0 20px; }
.gauge_box.line_chart{height:267px;}

.gauge_box .gauge_total{ display:flex; align-items:center; margin-bottom:35px; padding-left:95px; width:100%; height:50px;  }
.gauge_box .gauge_total>span{ display:inline-block; }
.gauge_total_box{ position:absolute;top:20px;left:20px;width:80px;height:50px;overflow:hidden; }
.gauge_total_box #chart_gauge svg g:nth-of-type(1){ display:none; }
.gauge_total_box #chart_gauge svg g:nth-of-type(2) g{ height:500px; }
.gauge_total_box #chart_gauge svg g:nth-of-type(2) path:first-child{  fill:url(#MyGradient) !important; }
.gauge_total_box #chart_gauge{ width:100px;height:100px;top: -10px;position: absolute;left: -10px; }


.gauge_box dl.graph_per dt{ display:block; padding-left:27px; width:100%; height:22px; margin:20px 0 10px; }
.gauge_box dl.graph_per dt:first-op-type{ margin-top:0; }
.gauge_box dl.graph_per dt.passed { background:url("../images/common/ico_passed.png") no-repeat; }
.gauge_box dl.graph_per dt.failed { background:url("../images/common/ico_failed.png") no-repeat; }
.gauge_box dl.graph_per dd { letter-spacing:-4px; font-size:0; }
.gauge_box dl.graph_per dd p { display:inline-block; letter-spacing:normal; }
.gauge_box dl.graph_per dd p.bar{ width:calc(100% - 60px); height:15px; border-radius:50px; overflow:hidden; }
.gauge_box dl.graph_per dd p.bar span{ display:block; height:100%;}
.gauge_box dl.graph_per dt.passed + dd p.bar { background:var(--low-t-color); }
.gauge_box dl.graph_per dt.passed + dd p.bar span { background:var(--low-color); border-radius:50px; }
.gauge_box dl.graph_per dt.failed + dd p.bar { background:var(--fail-t-color); }
.gauge_box dl.graph_per dt.failed + dd p.bar span { background:var(--fail-color); border-radius:50px; }
.gauge_box dl.graph_per dd p.txt_data{ width:60px; text-align:right; }
.gauge_box div.graph_per > div{ display:block; width:100%; letter-spacing:-4px; margin-top:10px; }
.gauge_box div.graph_per > div:first-of-type{ margin-top:0; }
.gauge_box div.graph_per > div p{ display:inline-block; width:calc(100% - 190px); letter-spacing:normal; }
.gauge_box div.graph_per > div p.label{ width:125px; }
.gauge_box div.graph_per > div p.bar{ width:calc(100% - 190px); }
.gauge_box div.graph_per > div p.bar > span{ display:table; width:100%; height:15px; }
.gauge_box div.graph_per > div p.bar > span > span{ height:100%; display:table-cell; background:var(--passed-color); border-radius:50px; cursor:pointer;  }
.gauge_box div.graph_per > div p.bar > span::after{ content:""; width:auto; height:100%;  display:table-cell; background:var(--fail-color); border-radius:50px;  }
.gauge_box div.graph_per > div p.txt_data{ width:60px; text-align:right; }









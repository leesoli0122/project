/**그래프**/

.dashboard_chart_donut_box { padding:0 15px; }
.donut-chart{ display:flex;  padding:20px; width:336px; height:170px; position:relative; background:var(--cont-color); border-radius:20px; }
.donut-chart .chart { position:relative; display:inline-block; width:130px; height:130px; display:block; margin:0; /*top:50%; left:50%; transform:translate(-50%, -50%);*/ transition: 0.3s;  }
.donut-chart .chart > div::before{ content:""; display:block; position:absolute; top:50%; left:50%; transform:translate(-50%, -50%); width:95%; height:95%; border-radius:50%; z-index:1;}
.donut-chart .chart > div { display:block; position:absolute; width:100%; height:100%;  top:50%; left:50%; transform:translate(-50%, -50%); }
.donut-chart .chart > div + div {  width:calc(80% - 5px); height:calc(80% - 5px); margin-top:0;  }
.donut-chart .chart > div + div + div {  width:calc(55% - 5px); height:calc(55% - 5px); margin-top:0;  }
.donut-chart .chart > div + div + div + div {  width:calc(30% - 5px); height:calc(30% - 5px); margin-top:0;  }
.donut-chart .chart > div + div + div::before { width:90%; height:90%; }
.donut-chart .chart > div + div + div > p::after { width:80%; height:80%; }
.donut-chart .chart > div + div + div + div > p::after { width:70%; height:70%; }

.donut-chart .chart > div > p { position:absolute; display:block; width:100%; height:100%; border-radius: 50%; z-index:2; transform: rotate( -45deg ); }
.donut-chart .chart > div > p span.value{ position:absolute; top:90%; right:0; z-index:10; padding:5px; min-width:33px; color:#fff; font-weight:600; transform: rotate( 45deg ); text-align:center; background:#252738c4; border:1px solid #ffffff77; border-radius:3px; cursor:pointer;}
.donut-chart .chart > div > p span.value:hover{ background:#000; }
.donut-chart .chart > div.none{  display:none; }
.donut-chart .chart > div > p::after{ content:""; display:block; position:absolute; top:50%; left:50%; transform:translate(-50%, -50%) ; width:85%; height:85%; border-radius:50%; z-index:2; }
.donut-chart .chart > div::before{ background:#2B2553; box-shadow: 0 0 0 1px #5B21FF inset; }
.donut-chart .chart > div > p::after {  background:#2B2553;   }


.donut-chart .chart_data { display:flex; margin-left:15px; width:calc( 100% - 145px ); flex-direction:column; justify-content:center; }
.donut-chart .chart_data dt.tit{  margin-bottom:10px; font-size:16px; font-weight:700; color:#fff; text-align:left; }
.donut-chart .chart_data dd { position:relative; margin-top:5px; padding-left:18px; width:100%; opacity:.8; cursor:pointer; }
.donut-chart .chart_data dd:first-child { margin-top:0; }
.donut-chart .chart_data dd::before{ content:""; position:absolute; top:2px; left:0; display:inline-block; width:12px; height:12px; background:var(--none-color); border-radius:10px;  }
.donut-chart .chart_data dd.malware::before{ background:var(--malware-color);  }
.donut-chart .chart_data dd.hight::before{ background:var(--hight-color);  }
.donut-chart .chart_data dd.critical::before{ background:var(--critical-color);  }
.donut-chart .chart_data dd.medum::before{ background:var(--medum-color);  }
.donut-chart .chart_data dd.low::before{ background:var(--low-color);  }

.donut-chart .chart_data dd.success::before{ background:var(--success-color);  }
.donut-chart .chart_data dd.fail::before{ background:var(--fail-color);  }
.donut-chart .chart_data dd.sc_success::before{ background:var(--sc-success-color);  }
.donut-chart .chart_data dd.sc_medum::before{ background:var(--sc-medum-color);  }
.donut-chart .chart_data dd.image::before{ background:var(--image-color);  }
.donut-chart .chart_data dd.assurance::before{ background:var(-assurance-color);  }



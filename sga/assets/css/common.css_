/* 컬러 */
:root {
	--card-color01:#191B1F;
	--btn-color-blue:#4863FC;
	--btn-color-bhover:#2035aa;
	--btn--color-dark:#242636;
	--cont-color:#242636;
	--card-boder-color:#3e3e3e;
	--border-color:#ffffff3b;
	--sel-color:#262A37;
	--sel-color-hover:#1A1D26;
	--normal-color:#ffffffab;


	 /* CSS 변수 사용 
	 color: var(--text-color01);  f
	 */
}

body { min-width:1600px; background:#000; color:#fff; }
body * {  font-family:"S-CoreDream"; font-size:14px; }


input[type=color], input[type=date], input[type=datetime-local], input[type=datetime],
	input[type=email], input[type=month], input[type=number], input[type=password],
	input[type=search], input[type=tel], input[type=text], input[type=time],
	input[type=url], input[type=week], select { background:var(--cont-color); border:1px solid #ffffff75; 
	border-radius:7px; font-family:"S-CoreDream"; }
input[type=text]:hover, input[type=search]:hover, input[type=number]:hover,  input[type=password]:hover,
input[type=text]:focus, input[type=search]:focus, input[type=number]:focus,  input[type=password]:focus { color:#fff; background:var(--sel-color-hover) !important; border:1px solid #fff !important; }
.dp_none{ display:none; }


::-webkit-scrollbar { width:5px; height:100px; }
::-webkit-scrollbar-thumb { background:#fff; border-radius:5px; }
::-webkit-scrollbar-track { background:#000; }

.overflow_h{ overflow:hidden; }

/**new dashboard 추가**/

.dashboard_box_cont .dashboard_cont>li{top:0 !important;}
.dashboard_box_cont .dashboard_cont>li.w_75 { width: calc(75% - 21px); margin: 20px 10px 0; }
.dashboard_box_cont .dashboard_cont>li.w_40 { width: calc(40% - 21px); margin: 20px 10px 0; }
.dashboard_box_cont .dashboard_cont>li.w_35 { width: calc(35% - 21px); margin: 20px 10px 0; }


.dashboard_box_cont .dashboard_cont>li.h_2n{ height:1022px; }
.dashboard_box_cont .dashboard_cont>li.h_2n:nth-child(2n) + li.w_25{ margin-top:-502px !important;  }


.top_cont { padding:10px 20px; width:100%; height:60px; background:transparent; border-bottom:1px solid #FFFFFF; }
.top_cont * { vertical-align:top; }
a.logo_link { display:inline-block; margin-right:20px; width:145px; border-right:2px solid #707070; }
.logo_hidden_box img{ width:auto;  }
.location { position:static; float:none; display:inline-block; width:auto; height:auto; padding:0;}
.top_cont h2 {  font-weight:600; font-size:15px; position:absolute; left:50%; top:50%; transform:translate(-50%, -50%); }
.location ul li { vertical-align:top; }
.location ul li:first-child { font-size:0; background:url("../images/common/home.png") no-repeat center; background-size:18px; width:18px;  }
.location ul li+li:before { content:""; display:inline-block; width:18px; height:18px; background:url("../images/common/arr.png") no-repeat center 8px; }
.location h2 { background:transparent; }
.user_box { position:absolute; right:0; top:0; }
.user_info { margin-right:10px; padding-top:18px; }
.top_dashboard,
.top_dashboard_link{ width:60px; height:60px; border-left:0;  }

/**left menu**/
header.header_box {  display:grid; align-items:center; position:fixed; top:75px; left:10px; bottom:15px; width:60px; height:auto; background:var(--btn--color-dark); border-radius:10px;   }
header.header_box.on{ width:250px; background:#184385; box-shadow:2px 0px 6px 2px #1a53ab; }
.header_box nav .lnb>li { padding:0 10px; margin-bottom:10px; opacity:.65; }
.header_box nav .lnb>li.active { opacity:1; }
.header_box nav .lnb>li:last-child { margin-bottom:0; }
.header_box nav .lnb>li>a, .header_box.on nav .lnb>li>a { padding-left:0; width:100%; height:40px; }
.header_box nav .lnb>li.active>a{ width:100%; background:var(--success-color); border-radius:10px; }
.header_box nav .lnb>li>a:hover { width:100%; background:#000; border-radius:10px; }
.header_box nav .lnb>li>a:after { top:7px; left:7px; width:25px !important; height:25px !important; }
.header_box nav .lnb>li>a.menu01:after { background:url("../images/common/gnb_icon1.png") no-repeat center; }
.header_box nav .lnb>li>a.menu02:after { background:url("../images/common/gnb_icon2.png") no-repeat center; }
.header_box nav .lnb>li>a.menu03:after { background:url("../images/common/gnb_icon3.png") no-repeat center; }50
.header_box nav .lnb>li>a.menu04:after { background:url("../images/common/gnb_icon4.png") no-repeat center; }
.header_box nav .lnb>li>a.menu05:after { background:url("../images/common/gnb_icon6.png") no-repeat center; }
.header_box nav .lnb>li>a.menu06:after { background:url("../images/common/gnb_icon5.png") no-repeat center; }

.header_box.on nav .lnb>li>a>span { height:40px; line-height:40px; padding-left:40px; font-weight:500;  }
.header_box.on nav .lnb>li.has_children>a>span:before,
.header_box.on nav .lnb>li.active.has_children li.active.has_children>a>span:before,
.header_box.on nav .lnb>li.has_children li.has_children>a>span:before,
.header_box.on nav .lnb>li .depth2>li .depth3 li a span::before{ display:none; }
.header_box.on nav .lnb>li.has_children>a>span:after { position:absolute; background: url(../images/common/ico_select.svg) no-repeat right center;
    background-size: 14px; }
.header_box.on nav .lnb>li .depth2>li{ padding:3px 10px; border-bottom:1px solid var(--border-color); }
.header_box.on nav .lnb>li .depth2>li a { padding:0; }
.header_box.on nav .lnb>li .depth2>li>a>span { font-size:13px; }
.header_box.on nav .lnb>li .depth2>li.has_children>a{ background:url("../images/common/ico_select.svg") no-repeat right center; }
.header_box.on nav .lnb>li.active li.active .depth3 { opacity:.65; }
.header_box.on nav .lnb>li .depth2>li .depth3 li a{ padding:3px 0; }
.header_box.on nav .lnb>li .depth2>li .depth3 li a span { font-size:12px; font-weight:400; color:#fff; }
.header_box.on nav .lnb>li .depth2>li .depth3 li a span::after{ content:""; display:inline-block; position:absolute; right:0; width:10px;height:10px; background:url("../images/common/ico_select.svg") no-repeat right center; transform:rotate(270deg); top:12px; }


/**tab**/
.dashboard .tab_box { position:relative; padding:7px 15px; margin-bottom:25px; height:45px; background:var(--card-color01); border-radius:20px; }
.dashboard .tab_box .tab .tab_lst { overflow:hidden; height:auto; }
.dashboard .tab_box .tab .tab_add { position:absolute; right:15px; top:50%; transform:translate(0, -50%);  width:30px; height:30px; }


.dashboard .tab_box .tab .tab_lst li{ opacity:.5;margin-right:7px; padding-right:54px; width:auto; min-width:150px; height:29px; border-radius:7px; border:1px solid #ffffff !important;  }
.dashboard .tab_box .tab .tab_lst li:hover a{ color:#fff; }
.dashboard .tab_box .tab .tab_lst li:hover,
.dashboard .tab_box .tab .tab_lst li.open{ opacity:1;background:var(--btn--color-dark); }
.dashboard .tab_box .tab .tab_lst li a{ color:#fff; }
.dashboard .tab_box .tab .tab_lst li .full{ right:25px; }
.dashboard .tab_box .tab .tab_lst li .tab_cls{ right:5px; }

.dashboard .tab_box .tab .tab_lst li .tab_link { padding:0 10px; width:100%; font-weight:600; line-height:29px; color:#fff; }
.dashboard .tab_box .tab .tab_lst li .full,
.dashboard .tab_box .tab .tab_lst li .tab_cls{ top:3px !important; padding:0 !important; width:22px !important; height:22px !important; border-radius:0; }
.dashboard .tab_box .tab .tab_lst li .full:after{ width:100% !important; height:100% !important; background:url("../images/common/ico_tab_a.svg") no-repeat center !important;  }
.dashboard .tab_box .tab .tab_lst li .tab_cls:after { width:100% !important; height:100% !important; background:url("../images/common/ico_tab_delet.png") no-repeat center !important;  }

.tab_box ul.tab_item{ margin:30px 0 15px; }
.tab_box ul.tab_item li{ display:inline-block; width:auto; }
.tab_box ul.tab_item li a{ display:block; padding:5px 15px; width:100%; height:35px; line-height:25px; color:var(--normal-color); background:var(--btn--color-dark); border:1px solid var(--card-boder-color); border-radius:7px; }
.tab_box ul.tab_item li.on a{ background:var(--btn-color-blue); border:1px solid var(--btn-color-blue); color:#fff; }

.dashboard_box { background:transparent; }
.dashboard_box_top { margin:0 10px; width:calc(100% - 20px); height:35px; border-bottom:0; }
.dashboard_box_top .dashboard_title,
.dashboard_box_top .dashboard_btn_box{ padding:0; }
.dashboard_box_top .dashboard_title h3 { font-size:24px; font-weight:700; }

.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_link .sel_box .nice-select::before{ content:""; display:block; width:18px; height:18px; background-image: ;  }
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_link{ margin:10px 0; width:34px; height:34px; background:var(--cont-color); border:1px solid #ffffff75; border-radius:4px; }
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_link.on { background:#000; border:1px solid #fff;  }
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_link .linktext{ display:block; margin:0; width:100%; height:100%; background:var(--cont-color) url("../images/common/ico_setting.svg") no-repeat center; opacity:.5; }
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_link.on:hover .linktext,
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_link.on .linktext{ background:#000 url("../images/common/ico_setting.svg") no-repeat center; opacity:1; }
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_link .linktext::after, 
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_link .linktext::before{ display:none; }

.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_list { right:-2px; border-radius:4px; background:#000; border:1px solid #fff; }
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .sel_box .nice-select.small { padding-left:30px; }
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .sel_box .nice-select.small::before { content:""; display:block; position:absolute; top:7px; left:7px; width:18px; height:18px; background:url("../images/common/ico_calendar.png") no-repeat;   }



/**버튼영역**/
.btn { position:relative; padding:0 7px 0 35px; min-width:70px; font-weight:400; border-radius:7px;  background:var(--btn-color-blue); border:1px solid var(--btn-color-blue) !important; vertical-align:top; color:#fff;  }
.btn.del::before{ content:""; display:block; position:absolute; top:3px; left:3px; width:25px; height:25px; background:url("../images/common/ico_delet.svg") no-repeat; }

.btn.bline { padding:0 10px; min-width:100px; line-height:34px; text-align:left; }
.dashboard .tab_box .tab .tab_add span { width:30px; height:30px; background:var(--btn-color-blue)url("../images/common/ico_plus.svg") no-repeat center !important; background-size:12px !important; border-radius:30px;  }
.dashboard .tab_box .tab .tab_add:hover span { box-shadow: inset 0 0 8px var(--btn-color-bhover); }
.btn.bline span { width:100%; display:block; }
.btn.bline span:after { top:6px; right:0; width:20px; height:20px; background:var(--btn-color-blue)url("../images/common/ico_plus.svg") no-repeat center !important; background-size:8px !important; border-radius:30px;  }
.dashboard_option_select_box { width:100%; margin:0 10px;}

/**버튼 호버**/
.sel_box .nice-select:hover,
.dashboard_box_cont .dashboard_cont_top .cont_btn_box .cont_btn_link:hover{ background:var(--sel-color-hover) !important; border:1px solid #fff !important; }

/**form**/
textarea,
textarea[readonly]{ background:var(--btn--color-dark); }



/**select**/
.dashboard_box_registry { margin:20px 0 0 0; }
.registry_title p { font-weight:500; }
.registry_select_box { width:auto; min-width:160px; }
.sel_box .nice-select { background:var(--cont-color); border:1px solid #ffffff75; }
.sel_box .nice-select.popup_sel { padding-right:40px; }
.sel_box .nice-select.open { background:#000 !important; border:1px solid #fff !important; }
.sel_box .nice-select.popup_sel:after { background:url("../images/common/ico_select.svg") no-repeat center; background-size:14px; }
.sel_box .nice-select.popup_sel.open .list { background:#000; border:1px solid #ffffff !important; border-top:0 !important; }

/*malware*/
.malware .chart > div::before{ background:var(--malware-t-color); box-shadow: 0 0 0 1px var(--malware-color) inset; }
.malware .chart > div > p,
.malware .chart > div > p span.value{ transform:rotate(0); }
.malware .chart > div > p::after { background:var(--malware-t-color);   }
.malware .chart > div > p span.value { top:50%; right:-20px; }
.malware p.annex span.malware::before { background:var(--malware-color);   }

/**vulnerabilities*/
.vulnerabilities .chart > div > p,
.vulnerabilities .chart > div > p span.value{ transform:rotate(0); }
.vulnerabilities .chart > div > p::after { background:#24413D;   }
.vulnerabilities .chart > div > p span.value { top:50%; right:-20px; }

.vulnerabilities .chart > div > p.hight-chart{ transform:rotate(45deg); }
.vulnerabilities .chart > div > p.hight-chart span.value{ transform:rotate(-45deg); }

.vulnerabilities .chart > div > p.critical-chart{ transform:rotate(180deg); }
.vulnerabilities .chart > div > p.critical-chart span.value{ top:25%; transform:rotate(0); }

.vulnerabilities .chart > div > p.medum-chart{ transform:rotate(0); }
.vulnerabilities .chart > div > p.medum-chart span.value{ top:0; left:80px; right:inherit; transform:rotate(90deg); }

.vulnerabilities .chart > div > p.low-chart{ transform:rotate(130deg); }
.vulnerabilities .chart > div > p.low-chart span.value{ top:0; left:80px; right:inherit; transform:rotate(90deg); }


.vulnerabilities .chart > div.low::before{ background:var(--low-t-color); box-shadow: 0 0 0 1px var(--low-color) inset; }
.vulnerabilities .chart > div.hight::before{ background:var(--hight-t-color); box-shadow: 0 0 0 1px var(--hight-color) inset; }
.vulnerabilities .chart > div.critical::before{ background:var(--critical-t-color); box-shadow: 0 0 0 1px var(--critical-color) inset; }
.vulnerabilities .chart > div.medum::before{ background:var(--medum-t-color); box-shadow: 0 0 0 1px var(--medum-color) inset; }
.vulnerabilities .chart > div > p.hight-chart::after{ background:var(--hight-t-color); }
.vulnerabilities .chart > div > p.critical-chart::after{ background:var(--critical-t-color); }
.vulnerabilities .chart > div > p.medum-chart::after{ background:var(--medum-t-color); }
.vulnerabilities .chart > div > p.low-chart::after{ background:var(--low-t-color); }
.vulnerabilities p.annex span.critical::before { background:var(--critical-color);;   }
.vulnerabilities p.annex span.hight::before{ background:var(--hight-color);   }
.vulnerabilities p.annex span.medum::before { background:var(--medum-color);   }


/**Scan*/
.scan .chart > div > p,
.scan .chart > div > p span.value{ transform:rotate(0); }
.scan .chart > div > p::after { background:#24413D;   }
.scan .chart > div > p span.value { top:50%; right:-20px; }

.scan .chart > div > p.success-chart{ transform:rotate(45deg); }
.scan .chart > div > p.success-chart span.value{  transform:rotate(-45deg); }

.scan .chart > div > p.fail-chart{ transform:rotate(-90deg); }
.scan .chart > div > p.fail-chart span.value{ top:0; left:80px; right:inherit; transform:rotate(90deg); }


.scan .chart > div.success::before{ background:var(--success-t-color); box-shadow: 0 0 0 1px var(--success-color) inset; }
.scan .chart > div.fail::before{ background:var(--fail-t-color); box-shadow: 0 0 0 1px var(--fail-color) inset; }
.scan .chart > div > p.success-chart::after{ background:var(--success-t-color); }
.scan .chart > div > p.fail-chart::after{ background:var(--fail-t-color); }
.scan p.annex span.success::before { background:var(--success-color);  }
.scan p.annex span.fail::before{ background:var(--fail-color);   }


/**scan Completion*/
.scan_completion .chart > div > p,
.scan_completion .chart > div > p span.value{ transform:rotate(0); }
.scan_completion .chart > div > p::after { background:#24413D;   }
.scan_completion .chart > div > p span.value { top:50%; right:-20px; }

.scan_completion .chart > div > p.sc-success-chart{ transform:rotate(45deg); }
.scan_completion .chart > div > p.sc-success-chart span.value{ right:0; transform:rotate(-45deg); }

.scan_completion .chart > div > p.sc-medum-chart{ transform:rotate(-90deg); }
.scan_completion .chart > div > p.sc-medum-chart span.value{ top:-10px; left:110px; right:inherit; transform:rotate(90deg); }


.scan_completion .chart > div.sc-success::before{ background:var(--sc-success-t-color); box-shadow: 0 0 0 1px var(--sc-success-color) inset; }
.scan_completion .chart > div.sc-medum::before{ background:var(--sc-medum-t-color); box-shadow: 0 0 0 1px var(--sc-medum-color) inset; }
.scan_completion .chart > div > p.sc-success-chart::after{ background:var(--sc-success-t-color); }
.scan_completion .chart > div > p.sc-medum-chart::after{ background:var(--sc-medum-t-color); }
.scan_completion p.annex span.sc-success::before { background:var(--sc-success-color);  }
.scan_completion p.annex span.sc-medum::before{ background:var(--sc-medum-color);   }


/**Image Assurance */
.image_assurance .chart > div > p,
.image_assurance .chart > div > p span.value{ transform:rotate(0); }
.image_assurance .chart > div > p::after { background:#24413D;   }
.image_assurance .chart > div > p span.value { top:50%; right:-20px; }

.image_assurance .chart > div > p.image-chart{ transform:rotate(45deg); }
